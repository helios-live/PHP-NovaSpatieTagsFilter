{
  "name": "helios-live/php-nova-spatie-tags-filter",
  "description": "A Laravel Nova filter.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "HeliosLive\\PhpNovaSpatieTagsFilter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HeliosLive\\PhpNovaSpatieTagsFilter\\FilterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}